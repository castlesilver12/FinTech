    "sdate = '2016-01-01'\n",
    "edate = '2020-06-30'\n",
    "ticker_list = [('kakao','035720'), ('naver','035420'), ('samsung','005930')]\n",
    "df_list = []\n",
    "df = pd.DataFrame()\n",
    "for ticker in ticker_list:\n",
    "    df[f\"{ticker[0]}\"] = stock.get_market_ohlcv(sdate, edate , ticker[1])[['종가']]\n",
    "    df_list.append(df)\n",
    "df = pd.concat(df_list)\n",
    "df.head()"
   ]
